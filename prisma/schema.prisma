generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://${DATABASE_USER}:${DATABASE_PASSWORD}@${DATABASE_URI}/${DATABASE_NAME}"
}

model Client {
  id           Int      @id @default(autoincrement())
  nome         String
  email        String
  telefone     String
  coordenadaX  Float
  coordenadaY  Float
}
